{
    "manifest_version": 2,

    "name": "Entertain Me",
    "description": "An extension to brighten your day with a Chuck Norris joke!",
    "version": "1.0",
    "background": {
        "page": "popup.html"
    },

    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html",
        "default_title": "Entertain Me"
    },
    "permissions": [
        "background",
        "https://api.icndb.com/"
    ]
}